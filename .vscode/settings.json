{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        ".vscode": true,
        ".idea": true,
        ".dart_tool": true,
        "build": true,
        "flutter_sudoku.iml": true,
        ".flutter-plugins": true,
        ".flutter-plugins-dependencies": true,
        "symbols": true,
        "android/.gradle": true,
        "ios/.symlinks": true,
        "ios/Pods": true,
        "**/*.g.dart": true
    },
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 12,
    "gitlens.mode.active": "zen",
    "dart.closingLabels": false,
    "dart.lineLength": 120,
    "editor.rulers": [120],
    "editor.semanticHighlighting.enabled": false
}